option_settings:
    "aws:elasticbeanstalk:application:environment":
        DJANGO_SETTINGS_MODULE: "adonate_api.settings"
        "PYTHONPATH": "/opt/python/current/app/adonate_api:$PYTHONPATH"

    "aws:elasticbeanstalk:container:python":
        WSGIPath: adonate_api/adonate_api/wsgi.py
        NumProcesses: 3
        NumThreads: 20

    "aws:elasticbeanstalk:container:python:staticfiles":
        "/static/": "static/"